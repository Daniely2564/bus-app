{{#if list}}
<ul class="list-group">
    {{#each list}}
    <li class="list-group-item d-flex justify-content-between align-items-center" style="font-size:150%">
        {{this.dest}}
        <span class="badge badge-primary badge-pill" style="font-size:110%">{{this.min}}</span>
    </li>
  {{/each}}
</ul>
{{/if}}